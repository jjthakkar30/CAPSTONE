<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"] - ParkView</title>
  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
  <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
  <link rel="stylesheet" href="~/ParkView.styles.css" asp-append-version="true" />

  <style>

    .navbar {
      transition: all 1s;
      z-index: 20;
    }

    .navbar-scrolled {
      background-color: #000;
    }

    header {
      z-index: 20;
    }

    #Title {
      margin-top: 10px;
    }

  </style>
</head>
<body>
  <header>
    <nav class="navbar navbar-expand-sm fixed-top navbar-light">
      <div class="container">
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbar1">
          <span class="navbar-toggler-icon"></span>
        </button>
        <a class="navbar-brand" asp-controller="Home" asp-action="Index" style="margin-left:330px;">
          <img id="logo-img" src="/Images/logo-light.png" alt="Bootstrap" width="100" height="100">
        </a>
        <a class="navbar-brand" asp-controller="Home" asp-action="Index"><p id="Title" style="width: 500px; font-family:'Times New Roman'; font-size:50px; text-align:center; font-weight:bolder">THE PARKVIEW</p></a>
        <div class="collapse navbar-collapse" id="navbar1">
          <div class="d-grid gap-2 d-md-flex justify-content-md-end" style="margin-left:100px;">
            @*<button class="btn btn-outline-primary btn-lg" type="button" style=" border-radius:20px;">SIGN UP</button>
            <button class="btn btn-primary btn-lg" type="button" style="background-color:#1c2331; border-radius:20px;">LOG IN</button>*@
            <partial name="_LoginPartial" />
          </div>
        </div>
      </div>
    </nav>
    @*<video autoplay loop muted plays-inline class="back-video"><source src="/Videos/hotel_video_3.mp4" type="video/mp4"></video>*@
    @*<img src="/Images/hotel_carousel_3.jpg"  alt="No Image" style="width:100%; height:100vh;"/>*@
