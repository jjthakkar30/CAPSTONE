    <div class="container-fluid">
    <a class="navbar-brand" href="#">
    <img src="/Images/parkview_logo_1.webp" alt="Logo" width="30" height="24" class="d-inline-block align-text-top">
    Bootstrap
    </a>
    </div>
    </nav>*@
  </header>

  <main role="main" class="pb-3 main">
    @RenderBody()
  </main>

  <!-- Remove the container if you want to extend the Footer to full width. -->
  <div>
    <!-- Footer -->
    <footer class="text-center text-lg-start text-white"
            style="background-color: #1c2331">
      <!-- Section: Links  -->
      <section class="">
        <br />
        <div class="container text-center text-md-start mt-5">
          <!-- Grid row -->
          <div class="row mt-3">
            <!-- Grid column -->
            <div class="col-md-3 col-lg-4 col-xl-3 mx-auto mb-4">
              <!-- Content -->
              <h6 class="text-uppercase fw-bold">ParkView</h6>
              <hr class="mb-4 mt-0 d-inline-block mx-auto"
                  style="width: 60px; background-color: #7c4dff; height: 2px" />
              <p>
                ParkView is the largest Luxury hotel chain in India. Spread across
                20 states, The ParkView group of Hotels is providing its customors a
                Luxurious Hospitality since 1980.
              </p>
            </div>
            <!-- Grid column -->
            <!-- Grid column -->
            <div class="col-md-2 col-lg-2 col-xl-2 mx-auto mb-4">
              <!-- Links -->
              <h6 class="text-uppercase fw-bold">TOP LOCATIONS</h6>
              <hr class="mb-4 mt-0 d-inline-block mx-auto"
                  style="width: 60px; background-color: #7c4dff; height: 2px" />
              <p>
                <a href="#!" class="text-white">Bangalore</a>
              </p>
              <p>
                <a href="#!" class="text-white">Chennai</a>
              </p>
              <p>
                <a href="#!" class="text-white">Mumbai</a>
              </p>
              <p>
                <a href="#!" class="text-white">Delhi</a>
              </p>
            </div>
            <!-- Grid column -->
            <!-- Grid column -->
            <div class="col-md-3 col-lg-2 col-xl-2 mx-auto mb-4">
              <!-- Links -->
              <h6 class="text-uppercase fw-bold">Useful links</h6>
              <hr class="mb-4 mt-0 d-inline-block mx-auto"
              <p>
                <a asp-controller="Home" asp-action="Services" class="text-white">Services</a>
              </p>
              <p>
                <a asp-controller="Home" asp-action="About" class="text-white">About</a>
              </p>
              <p>
                <a asp-controller="Home" asp-action="Contact" class="text-white">Contact us</a>
              </p>
            </div>
            <!-- Grid column -->
            <!-- Grid column -->
            <div class="col-md-4 col-lg-3 col-xl-3 mx-auto mb-md-0 mb-4">
              <!-- Links -->
              <h6 class="text-uppercase fw-bold">Developer Info</h6>
              <hr class="mb-4 mt-0 d-inline-block mx-auto"
                  style="width: 60px; background-color: #7c4dff; height: 2px" />
              <p> Jinil & Devwrat</p>
              <p> developers@parkview.com</p>
              <p> + 91 94082 55536</p>
              <p> + 91 87709 80200</p>
            </div>
            <!-- Grid column -->
          </div>
          <!-- Grid row -->
        </div>
      </section>

    </footer>
  </div>

  <footer class="footer text-muted" style="background-color:#1c2331">
    <div class="text-center" style="color:white;">
      &copy; 2023 - ParkView - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>


  </footer>



  <script src="~/lib/jquery/dist/jquery.min.js"></script>
  <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="~/js/site.js" asp-append-version="true"></script>


  <script>
    const navEl = document.querySelector('nav');
    var compTitle = document.getElementById('Title');
    var logo = document.getElementById('logo-img');
    window.addEventListener('scroll', () => {
      if (window.scrollY >= 100) {
        navEl.classList.add('navbar-scrolled');
        compTitle.style.color = "white";
        $("#logo-img").attr("src", "/Images/logo-dark.png");
      }
      else if (window.scrollY < 100) {
        navEl.classList.remove('navbar-scrolled');
        compTitle.style.color = "black";
        $("#logo-img").attr("src", "/Images/logo-light.png");
      }

    });
  </script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>